{
    "name": "tonystore/livewire-notification",
    "description": "Laravel package to launch toast notifications.",
    "type": "package",
    "license": "MIT",
    "keywords": [
        "tonystore",
        "livewire-notification",
        "livewire-toast"
    ],
    "autoload": {
        "psr-4": {
            "Tonystore\\LivewireNotification\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Anthony Medina Sandoval",
            "email": "thony918@outlook.com"
        }
    ],
    "require": {
        "php": "^7.2.5|^8.0",
        "livewire/livewire": "^2.0|^3.0@beta|^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tonystore\\LivewireNotification\\LivewireNotificationProvider"
            ],
            "aliases": {
                "LivewireNotification": "Tonystore\\LivewireNotification\\LivewireNotificationFacade"
            }
        }
    }
}
